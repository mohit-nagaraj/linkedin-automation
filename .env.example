# LinkedIn Credentials (Required)
LINKEDIN_EMAIL=your_email@example.com
LINKEDIN_PASSWORD=your_password

# Search Configuration (Required)
SEARCH_KEYWORDS=software engineer, founder, cto
# Optional: Filter by locations
# LOCATIONS=San Francisco, New York, Remote

# Google Gemini API (Required for AI features)
GOOGLE_API_KEY=your_gemini_api_key_here

# Google Sheets Configuration (Required)
GSHEET_ID=your_google_sheet_id_here
GCP_SERVICE_ACCOUNT_JSON_PATH=service_account.json
# Optional: Custom worksheet name (default: Leads)
# GSHEET_WORKSHEET=Leads

# Test Mode - IMPORTANT: Set to false to actually send connection requests
TEST_MODE=true

# Processing Limits
MAX_PROFILES=10

# Browser Configuration
HEADLESS=false
BROWSER_CHANNEL=chrome
USE_PERSISTENT_CONTEXT=true
USER_DATA_DIR=.playwright/user-data

# Debugging
DEBUG=false

# Rate Limiting (milliseconds)
MIN_ACTION_DELAY_MS=500
MAX_ACTION_DELAY_MS=2000

# Advanced Configuration
# SLOW_MO_MS=0
# NAVIGATION_TIMEOUT_MS=30000
# STORAGE_STATE_PATH=.playwright/storage_state.json

# OAuth Configuration (Alternative to service account)
# OAUTH_CLIENT_SECRETS_PATH=client_secrets.json
# OAUTH_TOKEN_PATH=token.json

# Seniority Keywords (for scoring)
# SENIORITY_KEYWORDS=founder,co-founder,cto,ceo,head,director,vp,lead,senior,principal